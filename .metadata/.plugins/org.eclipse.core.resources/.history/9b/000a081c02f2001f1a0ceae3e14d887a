#ifndef API_INC_RADAR_GPIO_H_
#define API_INC_RADAR_GPIO_H_

#include "Headers.h"

typedef uint16_t led_t;
typedef bool buttonStatus_t;

void writeLedOn_GPIO(led_t LDx);
